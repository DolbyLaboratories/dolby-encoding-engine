<?xml version="1.0"?>
<job_config>
    <input>
        <video>
            <hevc>
                <frame_rate>FRAME_RATE</frame_rate>
                <dv_profile>none</dv_profile>
                <file_name>FILE_NAME</file_name>
                <storage>
                    <local>
                        <path>PATH</path>
                    </local>
                </storage>
            </hevc>
        </video>
    </input>
    <filter>
        <video>
            <transcode_to_hevc>
                <encode_start>0</encode_start>
                <encode_end>0</encode_end>
                <target_width>0</target_width>
                <target_height>0</target_height>
                <data_rate>15000</data_rate>
                <max_vbv_data_rate>15000</max_vbv_data_rate>
                <vbv_buffer_size>30000</vbv_buffer_size>
                <encode_pass_num>1</encode_pass_num>
                <decoder>
                    <hevc_dec>
                        <impact>
                            <use_sps_frame_rate>false</use_sps_frame_rate>
                        </impact>
                    </hevc_dec>
                </decoder>
                <scaling>
                    <base/>
                </scaling>
                <hevc_enc>
                    <ffmpeg>
                        <ffmpeg_bin>/absolute/path/to/ffmpeg</ffmpeg_bin>
                        <cmd_gen>/absolute/path/to/cmd_gen_x265.py</cmd_gen>
                        <user_params_file>/absolute/path/to/transcode_to_hevc.json</user_params_file>
                    </ffmpeg>
                </hevc_enc>
            </transcode_to_hevc>
        </video>
    </filter>
    <output>
        <hevc>
            <file_name>FILE_NAME</file_name>
            <storage>
                <local>
                    <path>PATH</path>
                </local>
            </storage>
        </hevc>
    </output>
    <misc>
        <temp_dir>
            <clean_temp>true</clean_temp>
            <path>PATH</path>
        </temp_dir>
    </misc>
</job_config>
